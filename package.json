{
    "name": "nest-starter",
    "version": "0.0.1",
    "private": "true",
    "license": "UNLICENSED",
    "scripts": {
        "build:prod": "tsc -p tsconfig.build.json && cp ./src/modules/order/assets ./dist/src/modules/order -R",
        "start:hmr": "node dist/src/main.hmr.ts",
        "start:dev": "ts-node src/main.ts",
        "start:prod": "node dist/src/main.js",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli -d ormconfig.ts",
        "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/$npm_config_name",
        "migration:create": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli migration:create ./src/migrations/$npm_config_name",
        "migration:run": "npm run build && npm run typeorm -- migration:run",
        "migration:revert": "npm run typeorm -- migration:revert",
        "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
        "schema:drop": "yarn run typeorm schema:drop",
        "watch:dev": "nodemon --config nodemon.json",
        "debug:dev": "nodemon --config nodemon-debug.json",
        "webpack": "webpack --config webpack.config.js --progress",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint --fix . --ext .ts",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@nestjs/bull": "^0.6.2",
        "@nestjs/cache-manager": "^1.0.0",
        "@nestjs/common": "^9.0.0",
        "@nestjs/config": "^2.3.1",
        "@nestjs/core": "^9.0.0",
        "@nestjs/jwt": "^9.0.0",
        "@nestjs/microservices": "^9.2.0",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.0",
        "@nestjs/platform-socket.io": "^9.0.0",
        "@nestjs/serve-static": "^3.0.1",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/typeorm": "^9.0.1",
        "@nestjs/websockets": "^9.0.0",
        "axios": "^1.3.3",
        "bcrypt": "^5.1.0",
        "bull": "^4.10.1",
        "cache-manager": "^4",
        "cache-manager-redis-store": "2.0.0",
        "chalk": "^5.2.0",
        "chance": "^1.1.11",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "dotenv": "^16.0.3",
        "fs-extra": "^10.1.0",
        "jsonwebtoken": "^9.0.0",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.28",
        "morgan": "^1.10.0",
        "passport": "^0.6.0",
        "passport-custom": "^1.1.1",
        "passport-http-bearer": "^1.0.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.8.0",
        "random-email": "^1.0.3",
        "redis": "3.1.2",
        "reflect-metadata": "^0.1.13",
        "request-context": "^2.0.0",
        "request-ip": "^3.3.0",
        "rimraf": "^3.0.2",
        "rxjs": "^7.2.0",
        "swagger-ui-express": "^4.6.2",
        "typeorm": "^0.3.14",
        "typeorm-transactional-cls-hooked": "^0.1.21",
        "uuid": "^9.0.0",
        "yarn": "^1.22.19"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.0.0",
        "@nestjs/schematics": "^9.0.0",
        "@nestjs/testing": "^9.0.0",
        "@types/bull": "^4.10.0",
        "@types/cache-manager": "^4.0.2",
        "@types/express": "^4.17.13",
        "@types/jest": "28.1.8",
        "@types/node": "^16.0.0",
        "@types/passport-http-bearer": "^1.0.37",
        "@types/passport-jwt": "^3.0.8",
        "@types/supertest": "^2.0.11",
        "@types/uuid": "^9.0.0",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/eslint-plugin-tslint": "^5.57.1",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^8.0.1",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-simple-import-sort": "^10.0.0",
        "faker": "^5.5.3",
        "jest": "28.1.3",
        "nodemon": "^2.0.20",
        "prettier": "^2.3.2",
        "source-map-support": "^0.5.20",
        "supertest": "^6.1.3",
        "ts-jest": "28.0.8",
        "ts-loader": "^9.2.3",
        "ts-node": "^10.0.0",
        "tsconfig-paths": "4.1.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "tslint-consistent-codestyle": "^1.16.0",
        "tslint-eslint-rules": "^5.4.0",
        "typescript": "^4.7.4"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".spec.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-NODE_ENVd": {
        "*.ts": [
            "eslint --fix",
            "git add"
        ]
    }
}
